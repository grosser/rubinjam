#!/usr/bin/env ruby
# generated by rubinjam -- https://github.com/grosser/rubinjam
module Rubinjam
  LIBRARIES = {
    "hello/world" => "module Hello\n  WORLD = \"hello world\"\nend\n"
  }
end

def require(file)
  if code = Rubinjam::LIBRARIES[file]
    return if code == :loaded
    eval(code, TOPLEVEL_BINDING, "rubinjam/#{file}")
    Rubinjam::LIBRARIES[file] = :loaded
  else
    super
  end
end
#!/usr/bin/env ruby
require 'optparse'
require 'hello/world'
puts Hello::WORLD
